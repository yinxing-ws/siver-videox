  "description": "轻量级模板，使用 TypeScript，仅包含基础的 Layout。",
  "dependencies": {
    "@alifd/next": "^1.19.4",
    "@alifd/theme-design-pro": "0.x",
    "moment": "^2.24.0",
    "mp4box": "^0.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@ali/build-plugin-ice-def": "^0.1.0",
    "@commitlint/cli": "^16.2.1",
